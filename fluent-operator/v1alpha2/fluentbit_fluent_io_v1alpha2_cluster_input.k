    readFromHead?: bool

    refreshIntervalSeconds?: int

    rotateWaitSeconds?: int

    skipLongLines?: bool

    storageType?: "filesystem" | "memory"

    tag?: str

    tagRegex?: str


    check:
        regex.match(str(bufferChunkSize), r"^\d+(k|K|KB|kb|m|M|MB|mb|g|G|GB|gb)?$") if bufferChunkSize
        regex.match(str(bufferMaxSize), r"^\d+(k|K|KB|kb|m|M|MB|mb|g|G|GB|gb)?$") if bufferMaxSize
        regex.match(str(ignoredOlder), r"^\d+(m|h|d)?$") if ignoredOlder


