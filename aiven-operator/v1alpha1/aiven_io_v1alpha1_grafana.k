schema AivenIoV1alpha1GrafanaSpecUserConfigSMTPServer:
    """
    SMTP server settings

    Attributes
    ----------
    from_address : str, default is Undefined, required
        Address used for sending emails
    from_name : str, default is Undefined, optional
        Name used in outgoing emails, defaults to Grafana
    host : str, default is Undefined, required
        Server hostname or IP
    password : str, default is Undefined, optional
        Password for SMTP authentication
    port : int, default is Undefined, required
        SMTP server port
    skip_verify : bool, default is Undefined, optional
        Skip verifying server certificate. Defaults to false
    starttls_policy : str, default is Undefined, optional
        Either OpportunisticStartTLS, MandatoryStartTLS or NoStartTLS. Default is OpportunisticStartTLS.
    username : str, default is Undefined, optional
        Username for SMTP authentication
    """


    from_address: str

    from_name?: str

    host: str

    password?: str

    port: int

    skip_verify?: bool

    starttls_policy?: "OpportunisticStartTLS" | "MandatoryStartTLS" | "NoStartTLS"

    username?: str


    check:
        len(from_address) <= 319
        regex.match(str(from_address), r"^[A-Za-z0-9_\-\.+\'&]+@(([\da-zA-Z])([_\w-]{,62})\.){,127}(([\da-zA-Z])[_\w-]{,61})?([\da-zA-Z]\.((xn\-\-[a-zA-Z\d]+)|([a-zA-Z\d]{2,})))$")
        len(from_name) <= 128
        regex.match(str(from_name), r"^[^\x00-\x1F]+$") if from_name
        len(host) <= 255
        len(password) <= 255
        regex.match(str(password), r"^[^\x00-\x1F]+$") if password
        port <= 65535
        port >= 1
        len(username) <= 255
        regex.match(str(username), r"^[^\x00-\x1F]+$") if username


